{
  "keywords": [],
  "version": "0.0.0",
  "name": "@luismayta/emacs",
  "private": true,
  "dependencies": {
  },
  "engines": {
    "node": ">=12"
  },
  "publishConfig": {
    "access": "private"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@equipindustry/eslint-config": "^0.2.0",
    "@equipindustry/prettierrc": "^0.1.1",
    "@equipindustry/tsconfig": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "^2.22.0",
    "@typescript-eslint/parser": "^2.22.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-import": "^1.13.0",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-root-import": "^6.4.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^5.1.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint": "^6.8.0",
    "husky": "^4.2.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.0.4",
    "ts-jest": "^25.2.1",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:coverage": "yarn test --coverage",
    "lint": "eslint -c ./.eslintrc.js --ext .js,.jsx,.mjs,.d.ts,.ts,.tsx ./.",
    "lint:fix": "yarn fix --fix",
    "clean": "rimraf dist .next out",
    "clean:all": "yarn clean node_modules",
    "semantic-release": "semantic-release",
    "prettier": "prettier '**/*.{js?(on),ts?(x),scss,md,yml,yaml}' --write=false --list-different",
    "prettier:fix": "yarn prettier --write=true",
    "precommit": "lint-staged"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
},
"husky": {
  "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "yarn precommit"
  }
}
}
